day1: ../src/day1.c
	cc ../src/day1.c -o day1

day2: ../src/day2.hs
	ghc -o day2 ../src/day2

day3: ../src/day3.java
	java ../src/day3.java

day4: ../src/day4.py
	python ../src/day4.py

day5: ../src/day5.cs
	mcs -out:day5 ../src/day5.cs

day6: ../src/day6.cpp
	g++ -o day6 ../src/day6.cpp

clean:
	rm ../src/day2.o
	rm ../src/day2.hi